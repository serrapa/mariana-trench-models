{
    "model_generators": [
      {
        "find": "methods",
        "where": [
          {
            "constraint": "parent",
            "inner": {
              "constraint": "name",
              "pattern": "Landroid/app/PendingIntent;"
            }
          },
          {
            "constraint": "any_of",
            "inners": [
              {
                "constraint": "name",
                "pattern": "getB.*"
              },
              {
                "constraint": "name",
                "pattern": "getA.*"
              },
              {
                "constraint": "name",
                "pattern": "getS.*"
              }
            ]
          }
        ],
        "model": {
          "for_all_parameters": [
            {
              "variable": "x",
              "sinks": [
                {
                  "kind": "add_to_pending_intent",
                  "port": "Argument(x)"
                }
                ]
            }]
          },
          "verbosity" : 1
      }
    ]
  }