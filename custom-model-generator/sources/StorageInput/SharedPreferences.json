{
    "model_generators": [
      {
        "find" : "methods",
        "where" : [
          {
            "constraint": "parent",
            "inner": {
              "constraint": "name",
              "pattern": "Landroid/content/SharedPreferences;"
            }
          },
          { "constraint": "name", "pattern": "get.*" }
        ],
        "model" : {
          "modes": [
            "skip-analysis"
          ],
          "sources": [
            {
              "kind": "StorageInput",
              "port": "Return"
            }
          ]
        },
        "verbosity" : 1
    }]
  }